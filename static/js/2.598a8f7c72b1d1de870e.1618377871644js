                          return _context.stop();
                      }
                    }
                  }, _loop, _this4);
                });
                i = 0;

              case 3:
                if (!(i < _this4.datas.length)) {
                  _context2.next = 8;
                  break;
                }

                return _context2.delegateYield(_loop(i), "t0", 5);

              case 5:
                i++;
                _context2.next = 3;
                break;

              case 8:
              case "end":
                return _context2.stop();
            }
          }
        }, _callee, _this4);
      }))();
