        } else {
          console.log(this.formData.searchKeyWord);
          this.$store.commit("getSearchKeyWord_signUp01", this.formData.searchKeyWord);
          this.$store.commit("getSearchFromData_signUp", []);
        }
      }
      if (this.currentIndex == 1) {
