    // 提交编辑
    updataFromRequire: function updataFromRequire(activeRow) {
      var _this = this;

      this.$refs[activeRow].validate(function (valid) {
        _this.updateParam = new FormData();
        if (valid) {
          var _groupName = function _groupName(arr, id) {
            //通过ID获取groupName
            arr.forEach(function (item, index) {
              if (item.id == id) {
                name = item.name;
              } else if (item.children) {
                _groupName(item.children, id);
              }
            });
          };

          if (_this.DataDictionaryAllInfoShow.name !== _this.activeRow.name) {
            _this.updateParam.append("id", _this.activeRow.id);
            _this.updateParam.append("name", _this.activeRow.name);
            _this.updateParam.append("imageFile", _this.imageRaw);
          } else {
            _this.updateParam.append("id", _this.activeRow.id);
            _this.updateParam.append("imageFile", _this.imageRaw);
          }
          _this.$request.fieldUpdate(_this.updateParam).then(function (res) {
            if (res.status == 200 && res.data.code == "1000") {
              _this.dialogFormVisible = !_this.dialogFormVisible;
              //操作成功
              _this.$message({
                //弹窗
                type: "success",
                message: "更新成功!",
                duration: 1000
              });
              _this.closeUpdataFromRequire();
              _this.dialogVisible = false;
              _this.currentPage = 1;
              _this.getEquipmentList(); //刷新人员列表
            } else {
              _this.$message({
                type: "success",
                message: "更新成功!",
                duration: 1000
              });
              _this.dialogVisible = false;
              _this.closeUpdataFromRequire();
            }
          }).catch(function () {
            _this.$message({
              type: "error",
              message: "请求超时，请稍后重试!"
            });
            _this.dialogVisible = false;
            _this.dialogFormVisible = !_this.dialogFormVisible;
            _this.closeUpdataFromRequire();
          });
        } else {
          _this.$message.error("请检查信息是否填写完毕");
          return false;
        }
      });
    },

    //取消修改用户信息
    closeUpdataFromRequire: function closeUpdataFromRequire() {
      this.$refs["activeRow"].resetFields();
      this.dialogFormVisible = false;
    },

    //编辑图片变化
    imageUpdata: function imageUpdata(file, fileList, name) {
      var isJPG = file.raw.type === "image/jpeg";
      var isPNG = file.raw.type === "image/png";
      var isLt3M = file.size / 1024 / 1024 < 3;
      if (!(isJPG || isPNG)) {
        this.$message({
          type: "warning",
          message: "请选择格式为jpeg或png格式的文件",
          duration: 1000
        });
        return false;
      }
      if (!isLt3M) {
        this.$message({
          type: "warning",
          message: "图片大小不得超过3M！",
          duration: 1000
        });
        return false;
      }
      //将原有的照片清除掉
      this.updateImageUrl = "";
      this.showAddPic = false;
      var windowURL = window.URL || window.webkitURL;
      this.imgName = URL.createObjectURL(file.raw);
      this.activeRow.imageName = this.imgName;
      this.imageRaw = file.raw;
    },


    /****************************  查询&分页  **************************** */
    //前端渲染分页
    dealList: function dealList(list) {
      console.log(list);
      console.log(this.pageSize);
      console.log(this.currentPage);
      this.tableData = list.slice((this.currentPage - 1) * this.pageSize, this.currentPage * this.pageSize);
    },
    handleSizeChange: function handleSizeChange(val) {
      this.pageSize = val;
      this.currentPage = 1;
      this.getEquipmentList();
    },

    //调转的每页查询
    directHandleCurrentChange: function directHandleCurrentChange(val) {
      this.currentPage = val;
      this.getEquipmentList();
