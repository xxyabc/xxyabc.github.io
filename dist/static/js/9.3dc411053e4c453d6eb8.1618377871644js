          _this2.$message({
            type: "error",
            message: "请求超时，请稍后重试！",
            duration: 1000
          });
        });
      }).catch(function () {});
    },


    /*************  请求案场列表   ******************/
    getEquipmentList: function getEquipmentList(value) {
      var _this3 = this;

      this.loading = true;
      //后台请求列表接口
      this.$request.searchAnchang({
        name: this.serch,
        admin: this.rootAdminInfo
      }).then(function (response) {
        var res = response.data;
        console.log(res);
        if (res.code == "1000") {
          _this3.loading = false;
          var list = res.data.flatOrgs;
          _this3.total = list.length;
          _this3.dealList(list);
        } else {
          _this3.tableData = [];
          _this3.total = 0;
          _this3.loading = false;
        }
      }).catch(function (err) {
        _this3.$message({
          type: "error",
          message: "请求超时，请稍后重试!",
          duration: 1000
        });
        console.log("err=", err);
      });
    },


    /********************************* 服务配置 ********************/
    setService: function setService(index, row) {
      var _this4 = this;

      this.activeRow = row;
      this.sideIp = "";
      this.nowdata = "";
      this.nowin = "";
      this.nowmodel = "";
      this.$request.getServiceConfigs({
        anchangId: row.id
      }).then(function (response) {
        var res = response.data;
        console.log(res);
        if (res.code == "1000") {
          _this4.SEdialog = true;
          _this4.sideIp = res.data.marginIp ? res.data.marginIp.replace("https://", "") : "";
          _this4.radio = res.data.marginVersion ? res.data.marginVersion : "";
        } else {
          _this4.$message({
            type: "error",
