    /***************************  删除   ******************/
    handleDelete: function handleDelete(index, row) {
      var _this2 = this;

      this.$confirm("确认删除该案场?", "提示", {
        confirmButtonText: "确定",
        cancelButtonText: "取消",
        type: "warning"
      }).then(function () {
        //点击确定执行删除方法
        var data = {
          id: row.id
        };
        _this2.$request.delAnchang(data).then(function (res) {
          if (res.status == 200 && res.data.code == 1000) {
            _this2.$message({
              type: "success",
              message: "案场删除成功！",
              duration: 1000
            });
            _this2.getEquipmentList();
          } else {
            _this2.$message({
              type: "error",
