    },


    /**************************** 风控管理 ************************/
    //改节点个数
    changeNum: function changeNum(value, oldValue) {
      if (value - oldValue >= 0) {
        for (var i = 0; i < value - oldValue; i++) {
          var obj = {
            signName: "",
            signExpiry: ""
          };
          this.checks.push(obj);
        }
      } else {
        this.checks.pop();
      }
    },
    setMulti: function setMulti(index, row) {
      var _this6 = this;

      this.anchangId = row.id;
      //取核验配置
      this.$request.getMultiConfig({
        anchangId: this.anchangId
      }).then(function (response) {
        var res = response.data;
        if (res.code == "1000") {
          _this6.Multidialog = true;
          if (res.data.signCount == 1) {
            _this6.radioMulti = "1";
            _this6.checkNum = 2;
            _this6.checks = [{
              signName: "",
              signExpiry: ""
            }, {
              signName: "",
              signExpiry: ""
            }];
          } else {
            _this6.radioMulti = "2";
            _this6.checkNum = res.data.signCount;
            _this6.checks = [];
            for (var i = 0; i < res.data.signCount; i++) {
              _this6.checks.push({
                signName: res.data.signNodes[i].signName,
                signExpiry: res.data.signNodes[i].signExpiry
              });
            }
          }
        }
      });
      //取竞争带看配置
      this.$request.getCompeteConfig({
        anchangId: this.anchangId
      }).then(function (response) {
        var res = response.data;
        if (res.code == "1000") {
          _this6.radioCompete = res.data.allowManyReport ? "1" : "0";
        }
      });
    },
    closeMultidialog: function closeMultidialog() {
      this.Multidialog = false;
    },
    setMultiYes: function setMultiYes() {
      var _this7 = this;

      console.log("核验方式：", this.radioMulti);
      console.log("核验次数：", this.checkNum);
      console.log("间隔有效期：", this.checks);

      var param = {};
      if (this.radioMulti == "1") {
        param = {
          anchangId: this.anchangId,
          signCount: 1
        };
      } else {
        for (var i = 0; i < this.checks.length; i++) {
          if (this.checks[i].signName == "") {
            this.$message({
              type: "warning",
              message: "节点名称不能为空!",
              duration: 1000
            });
            return;
          }
          if (this.checks[i].signExpiry == "") {
            this.$message({
              type: "warning",
              message: "节点有效期不能为空!",
              duration: 1000
            });
            return;
          }
          if (!/(^[1-9]\d*$)/.test(this.checks[i].signExpiry)) {
            this.$message({
              type: "warning",
              message: "节点有效期应为正整数!",
              duration: 1000
            });
            return;
          }
        }

        this.checks.forEach(function (item, i) {
          item.index = i + 1;
        });
        param = {
          anchangId: this.anchangId,
          signCount: this.checkNum,
          signNodes: this.checks
        };
      }
      //保存核验配置
      this.$request.saveMultiConfig(param).then(function (response) {
        _this7.Multidialog = false;
        var res = response.data;
        if (res.code == "1000") {
          //保存竞争带看
          _this7.$request.saveCompeteConfig({
            anchangId: _this7.anchangId,
            allowManyReport: _this7.radioCompete == "1" ? true : false
          }).then(function (response) {
            var res = response.data;
            if (res.code == "1000") {
              _this7.$message({
                type: "success",
                message: "配置成功!",
                duration: 1000
              });
            } else {
              _this7.$message({
                type: "error",
                message: res.msg,
                duration: 1000
              });
            }
          });
        } else {
          _this7.$message({
            type: "error",
            message: res.msg,
            duration: 1000
          });
        }
      }).catch(function (err) {
        console.log(err);
        _this7.$message({
          type: "error",
          message: "请求超时，请稍后重试!",
          duration: 1000
        });
      });
    },
    checkNumber: function checkNumber(content) {
      if (!/(^[1-9]\d*$)/.test(content)) {
        this.$message({
          type: "warning",
          message: "请输入正整数!",
          duration: 1000
        });
      }
    }
  }
